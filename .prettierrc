{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^react$",
    "^next$",
    "^next/*",
    "<THIRD_PARTY_MODULES>",
    "^@/*",
    "^[.]",
    "^/",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ]
}
